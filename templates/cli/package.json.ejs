{
  "name": "<%= projectName %>",
  "version": "0.1.0",
  "description": "<%= description %>",
  "type": "module",
  "main": "./dist/cli.js",
  "bin": {
    "<%= projectName %>": "./dist/cli.js"
  },
  "files": [
    "dist/"
  ],
<% if (githubUrl) { %>
  "repository": {
    "type": "git",
    "url": "<%= githubUrl %>.git"
  },
  "homepage": "<%= githubUrl %>#readme",
  "bugs": {
    "url": "<%= githubUrl %>/issues"
  },
<% } %>
  "scripts": {
    "dev": "bun run src/cli.ts",
    "build": "tsc",
    "build:binary": "bun build src/cli.ts --compile --outfile <%= projectName %>",
    "build:darwin-arm64": "mkdir -p dist && bun build src/cli.ts --compile --target=bun-darwin-arm64 --outfile dist/<%= projectName %>-darwin-arm64",
    "build:darwin-x64": "mkdir -p dist && bun build src/cli.ts --compile --target=bun-darwin-x64 --outfile dist/<%= projectName %>-darwin-x64",
    "build:linux-x64": "mkdir -p dist && bun build src/cli.ts --compile --target=bun-linux-x64 --outfile dist/<%= projectName %>-linux-x64",
    "build:linux-arm64": "mkdir -p dist && bun build src/cli.ts --compile --target=bun-linux-arm64 --outfile dist/<%= projectName %>-linux-arm64",
    "build:windows-x64": "mkdir -p dist && bun build src/cli.ts --compile --target=bun-windows-x64 --outfile dist/<%= projectName %>-windows-x64.exe",
    "build:binaries": "bun run build:darwin-arm64 && bun run build:darwin-x64 && bun run build:linux-x64 && bun run build:linux-arm64 && bun run build:windows-x64",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "oxlint": "oxlint .",
    "typecheck": "tsc --noEmit",
    "check": "bun run typecheck && bun run lint && bun run oxlint",
    "check:all": "bun run check && bun run test:coverage"<% if (hasAddon('docs')) { %>,
    "docs": "typedoc",
    "docs:watch": "typedoc --watch"<% } %>
  },
  "keywords": [
    "cli",
    "typescript",
    "bun"
  ],
  "author": "<%= author %>",
  "license": "<%= license %>",
  "engines": {
    "bun": ">=1.0.0"
  },
<% const deps = []; if (hasAddon('zod')) deps.push('"zod": "^3.24.0"'); if (hasAddon('neverthrow')) deps.push('"neverthrow": "^8.2.0"'); %>  "dependencies": {<% if (deps.length > 0) { %>
<%= deps.map(d => '    ' + d).join(',\n') %>
  <% } %>},
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^20.0.0",
    "bun-types": "latest",
    "lefthook": "^2.0.13",
    "oxlint": "^1.38.0",
    "typescript": "^5.0.0"<% if (hasAddon('docs')) { %>,
    "typedoc": "^0.27.0"<% } %>
  }
}
