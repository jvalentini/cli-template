{
  "name": "<%= projectName %>",
  "version": "0.1.0",
  "description": "<%= description %>",
  "type": "module",
  "bin": {
    "<%= projectName %>": "./dist/cli.js"
  },
  "scripts": {
    "dev": "bun run src/cli.ts",
    "start": "bun run dist/cli.js",
    "build": "bun build src/cli.ts --outdir dist --target bun",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check": "bun run typecheck && bun run lint && bun run test"<% if (hasAddon('docs')) { %>,
    "docs": "typedoc",
    "docs:serve": "typedoc && bunx serve docs"<% } %><% if (hasAddon('security')) { %>,
    "security": "trivy fs --scanners vuln,secret .",
    "audit": "bun pm audit"<% } %>
  },
  "dependencies": {
    "effect": "^3.12.0",
    "@effect/cli": "^0.54.0",
    "@effect/platform": "^0.75.0",
    "@effect/platform-bun": "^0.56.0",
    "@effect/schema": "^0.75.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/bun": "^1.1.0",
    "lefthook": "^1.9.0",
    "knip": "^5.0.0",
    "typescript": "^5.7.0"<% if (hasAddon('docs')) { %>,
    "typedoc": "^0.27.0"<% } %>
  },
  "author": "<%= author %>",
  "license": "<%= license %>",
  "keywords": [
    "cli",
    "effect",
    "effect-ts",
    "typescript",
    "bun",
    "functional"
  ]<% if (githubUsername) { %>,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= githubUsername %>/<%= projectName %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= githubUsername %>/<%= projectName %>/issues"
  },
  "homepage": "https://github.com/<%= githubUsername %>/<%= projectName %>#readme"<% } %>
}
