<% if (webFramework === 'react-vite') { -%>
{
  "name": "@<%= projectName %>/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"<% if (hasAddon('tanstack-query')) { %>,
    "@tanstack/react-query": "^5.62.0"<% } %><% if (hasAddon('tanstack-router')) { %>,
    "@tanstack/react-router": "^1.91.0"<% } %><% if (hasAddon('tanstack-form')) { %>,
    "@tanstack/react-form": "^0.41.0"<% } %>
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "vite": "^6.0.0"
  }
}
<% } else if (webFramework === 'nextjs') { -%>
{
  "name": "@<%= projectName %>/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  }
}
<% } else if (webFramework === 'vue') { -%>
{
  "name": "@<%= projectName %>/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.5.0"<% if (hasAddon('tanstack-query')) { %>,
    "@tanstack/vue-query": "^5.62.0"<% } %><% if (hasAddon('tanstack-router')) { %>,
    "@tanstack/vue-router": "^1.91.0"<% } %>
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.0",
    "vite": "^6.0.0",
    "vue-tsc": "^2.2.0"
  }
}
<% } else if (webFramework === 'tanstack-start') { -%>
{
  "name": "@<%= projectName %>/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.91.0",
    "@tanstack/start": "^1.91.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vinxi": "^0.5.0"<% if (hasAddon('tanstack-query')) { %>,
    "@tanstack/react-query": "^5.62.0"<% } %><% if (hasAddon('tanstack-form')) { %>,
    "@tanstack/react-form": "^0.41.0"<% } %>
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  }
}
<% } -%>
