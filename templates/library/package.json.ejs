{
  "name": "<%= projectName %>",
  "version": "0.1.0",
  "description": "<%= description %>",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
<% if (githubUrl) { %>
  "repository": {
    "type": "git",
    "url": "<%= githubUrl %>.git"
  },
  "homepage": "<%= githubUrl %>#readme",
  "bugs": {
    "url": "<%= githubUrl %>/issues"
  },
<% } %>
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc && bun run build:cjs",
    "build:cjs": "bun build src/index.ts --outfile dist/index.cjs --target=node",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "oxlint": "oxlint .",
    "typecheck": "tsc --noEmit",
    "check": "bun run typecheck && bun run lint && bun run oxlint",
    "check:all": "bun run check && bun run test:coverage"<% if (hasAddon('docs')) { %>,
    "docs": "typedoc",
    "docs:watch": "typedoc --watch"<% } %>
  },
  "keywords": [
    "typescript",
    "bun",
    "library"
  ],
  "author": "<%= author %>",
  "license": "<%= license %>",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
<% const deps = []; if (hasAddon('zod')) deps.push('"zod": "^3.24.0"'); if (hasAddon('neverthrow')) deps.push('"neverthrow": "^8.2.0"'); if (hasAddon('effect')) deps.push('"effect": "^3.12.0"'); %>  "dependencies": {<% if (deps.length > 0) { %>
<%= deps.map(d => '    ' + d).join(',\n') %>
  <% } %>},
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^20.0.0",
    "bun-types": "latest",
    "lefthook": "^2.0.13",
    "oxlint": "^1.38.0",
    "typescript": "^5.0.0"<% if (hasAddon('docs')) { %>,
    "typedoc": "^0.27.0"<% } %>
  }
}
